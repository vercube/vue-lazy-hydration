{
  "name": "@vercube/nuxt-lazy-hydration",
  "version": "1.3.0",
  "description": "Optimize Vue 3 SSR with lazy hydration, delaying HTML hydration until it's needed for better performance",
  "sideEffects": false,
  "author": {
    "name": "Oskar Lebuda",
    "email": "o.lebuda@gmail.com"
  },
  "license": "MIT",
  "module": "./dist/index.esm.js",
  "main": "./dist/index.cjs.js",
  "types": "./dist/index.esm.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "module": "./dist/index.esm.js",
      "types": "./dist/index.esm.d.ts",
      "import": "./dist/index.esm.js",
      "default": "./dist/index.cjs.js"
    }
  },
  "engines": {
    "node": ">=18.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OskarLebuda/vue-lazy-hydration.git"
  },
  "bugs": {
    "url": "https://github.com/OskarLebuda/vue-lazy-hydration/issues"
  },
  "homepage": "https://github.com/OskarLebuda/vue-lazy-hydration#readme",
  "keywords": [
    "vue",
    "vue3",
    "lazy-hydration",
    "hydration",
    "ssr",
    "server-side-rendering",
    "performance",
    "vue-ssr",
    "lazy-load",
    "vue-hydration",
    "nuxt",
    "nuxtjs",
    "nuxt3"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "peerDependencies": {
    "vue": ">=3.5.13",
    "nuxt": ">=3.14"
  },
  "dependencies": {
    "@vercube/vue-lazy-hydration": "1.3.0",
    "@nuxt/kit": "4.3.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "1.0.2",
    "@nuxt/schema": "4.3.0",
    "vue": "3.5.27"
  }
}
